{
	"name": "TuleapIntegration",
	"version": "",
	"license-name": "GPL-3.0-only",
	"author": [
		"[http://www.hallowelt.com Hallo Welt! GmbH]"
	],
	"url": "http://www.hallowelt.com",
	"descriptionmsg": "tuleapintegration-desc",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"MessagesDirs": {
		"TuleapIntegration": "i18n"
	},
	"AutoloadNamespaces": {
		"TuleapIntegration\\": "src/",
		"TuleapIntegration\\Tests\\": "tests/phpunit"
	},
	"config": {
		"TuleapUrl": {
			"value": "https://tuleap-web.tuleap-aio-dev.docker"
		},
		"TuleapOAuth2Config": {
			"value": {
				"clientId": "",
				"clientSecret": "",
				"redirectUri": ""
			}
		}
	},
	"Hooks": {
		"BeforeInitialize": "redirectToLogin",
		"PersonalUrls": [ "removeManualLogin", "replaceUserLinks" ],
		"HtmlPageLinkRendererEnd": "replaceUserLinks"
	},
	"HookHandlers": {
		"redirectToLogin": {
			"class": "TuleapIntegration\\Hook\\RedirectToLogin"
		},
		"removeManualLogin": {
			"class": "TuleapIntegration\\Hook\\RemoveManualLogin"
		},
		"addEntityLinks": {
			"class": "TuleapIntegration\\Hook\\AddEntityLinks",
			"services": [ "MainConfig" ]
		},
		"replaceUserLinks": {
			"class": "TuleapIntegration\\Hook\\ReplaceUserLinks",
			"services": [ "UserFactory" ]
		}
	},
	"SpecialPages": {
		"TuleapLogin": {
			"class": "TuleapIntegration\\Special\\TuleapLogin",
			"services": [ "TuleapConnection", "TitleFactory", "UserFactory", "UserOptionsManager" ]
		}
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"manifest_version": 2
}
